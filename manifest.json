{
    "name" : "test",
    "version": "1",
    "manifest_version": 2,
    "permissions": ["storage","tabs"],
    "content_scripts": [
        {
            "matches" : ["<all_urls>"],
            "js": ["main.js"],
            "html": ["content.html"]
        }
    ],

    "browser_action" :{
        "default_popup" : "content.html",
        "default_title": "Test"
    }


}